{
    "name": "zepfietje/filament-polish",
    "description": "Opinionated improvements to the Filament UI.",
    "keywords": [
        "laravel",
        "filament",
        "polish"
    ],
    "homepage": "https://github.com/zepfietje/filament-polish",
    "license": "MIT",
    "authors": [
        {
            "name": "Zep Fietje",
            "email": "hey@zepfietje.com",
            "homepage": "https://zepfietje.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "filament/filament": "^2.0",
        "spatie/laravel-package-tools": "^1.9.2",
        "illuminate/contracts": "^9.0"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "nunomaduro/collision": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "ZepFietje\\FilamentPolish\\": "src"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "ZepFietje\\FilamentPolish\\FilamentPolishServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
